      <section id="id1171088125425">
        <title>Harvey the household robot</title>
        <figure id="id1171088118635">
          <media id="id1171088118635_media" alt="">
            <image mime-type="image/png" src="../../media/Picture 2-d821.png" id="id1171088118635__onlineimage" height="266" width="341"/>
